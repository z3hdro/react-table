{
  "extends": "airbnb",
  "plugins": ["react"],
  "globals": {
    "window": true,
    "localStorage": true,
    "document": true,
    "navigator":  true,
    "self": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
    }
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": 0,
    "no-param-reassign": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx", ".tsx", ".ts"] }],
    "react/prop-types": "off",
    "import/no-named-as-default": 0,
    "import/extensions": "off",
    "import/no-unresolved": "off",
    "react/jsx-props-no-spreading": "off",
    "no-nested-ternary": "off"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
          ".js",
          ".jsx",
          ".ios.js"
        ]
      }
    }
  }
}
